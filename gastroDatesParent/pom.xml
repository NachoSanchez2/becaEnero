<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>edu.beca.es.eoi</groupId>
	<artifactId>gastroDatesParent</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>gastroDatesParent</name>
	<description>gastroDatesParent</description>
	<modules>
		<module>gastroDatesUI</module>
		<module>gastroDatesServices</module>
		<module>gastroDatesData</module>
		<module>gastroDatesController</module>
	</modules>
	<properties>
		<lombok.version>1.18.16</lombok.version>
		<log4j.version>1.2.17</log4j.version>
		<junit.version>5.3.1</junit.version>
		<apache.poi.version>3.15</apache.poi.version>
		<controller.version>0.0.1-SNAPSHOT</controller.version>
		<services.version>0.0.1-SNAPSHOT</services.version>
		<data.version>0.0.1-SNAPSHOT</data.version>
	</properties>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.projectlombok</groupId>
				<artifactId>lombok</artifactId>
				<version>${lombok.version}</version>
			</dependency>
			<dependency>
				<groupId>log4j</groupId>
				<artifactId>log4j</artifactId>
				<version>${log4j.version}</version>
			</dependency>
			<dependency>
				<groupId>org.junit.jupiter</groupId>
				<artifactId>junit-jupiter-engine</artifactId>
				<version>${junit.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi</artifactId>
				<version>${apache.poi.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.poi</groupId>
				<artifactId>poi-ooxml</artifactId>
				<version>${apache.poi.version}</version>
			</dependency>
			<dependency>
				<groupId>edu.beca.es.eoi</groupId>
				<artifactId>gastroDatesController</artifactId>
				<version>${controller.version}</version>
			</dependency>
			<dependency>
				<groupId>edu.beca.es.eoi</groupId>
				<artifactId>gastroDatesServices</artifactId>
				<version>${services.version}</version>
			</dependency>
			<dependency>
				<groupId>edu.beca.es.eoi</groupId>
				<artifactId>gastroDatesData</artifactId>
				<version>${data.version}</version>
			</dependency>

		</dependencies>
	</dependencyManagement>

	<dependencies>
		<dependency>
			<groupId>org.junit.jupiter</groupId>
			<artifactId>junit-jupiter-engine</artifactId>
			<scope>test</scope>
		</dependency>
	</dependencies>

	<build>
		<finalName>maven-unit-test</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.22.0</version>
			</plugin>
		</plugins>
	</build>

</project>